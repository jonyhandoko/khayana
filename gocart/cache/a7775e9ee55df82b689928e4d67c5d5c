	<script type="application/javascript">
	var test = 0;
	
		function submitform()
		{
			document.myform.submit();
		}
		
		function insert_notification()
		{
			if($('#customer_email').val()==''){
				alert('Please input email');
			}else{
				$('.notification_container').hide();
				$('.message_notification').hide();

				var cust = $('#customer_email').val();
				//remove the shipping and payment forms
				
				
				$('.notification_container').html('<img alt="loading" src="http://localhost:8080/tokita/images/ajax-loader.gif"/></div>').show();
				$.post('http://localhost:8080/tokita/cart/insert_notification',{email: cust, product:3}, function(data){
					//populate the form with their information
					if(data!=1){
						$('.notification_container').html(data);
					}else{
						$('.notification_container').html('<input type="text" id="customer_email" value=""/><input type="button" class="button2" value="Notify Us" onClick="insert_notification()" />');
						$('.message_notification').html('email invalid').show();
					}
					//$('input:button, input:submit, button').button();		
				});
			}
			
			
		}
	
		$(document).ready(function(){
			$('.jqzoom').jqzoom({
				zoomType: 'standard',
				lens:false,
				zoomWidth: 425,
				xOffset: 0,
				title: false,
				zoomHeight: 416,  
				preloadImages: false,
				alwaysOn:false
			});
			
			$("a#fancybox-buttons").fancybox({
				openEffect	: 'elastic',
				closeEffect	: 'elastic'
			});
			
			$('a#fancybox-buttons-1').click(function(event){ 
				event.preventDefault();
				$('.detail-thumb').addClass('fancybox');
				$.fancybox.open($('a#fancybox-buttons'), {openEffect	: 'elastic',
				closeEffect	: 'elastic',index : test});        
			});    
			
			
			$('.size').change(function(){
				var qty = $(this).find(':selected')[0].id;

				$(".quantity").empty();
				$('.quantity').fadeIn('fast');
				$(".quantity").append("<span id='loading'></span>");
				$('.quantity').load('http://localhost:8080/tokita//gocart/themes/scarlet/views/dropdownAJAX.php?check='+qty);
						
			});
		});
	</script>
    
